package com.mycompany.product.DTO;

import lombok.Getter;
import lombok.Setter;

@Getter
@Setter
public class ProductCalculator {
	private Integer a;
	private Integer b;
	
	
	public ProductCalculator(Integer a, Integer b) {
		super();
		this.a = a;
		this.b = b;
	}


	@Override
	public String toString() {
		return "ProductCalculator [a=" + a + ", b=" + b + "]";
	}
	
}
